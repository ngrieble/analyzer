                function keysbyValue(O){
                    var A= [];
                    for(var p in O){
                        if(O.hasOwnProperty(p)) A.push([p, O[p]]);
                    }
                    A.sort(function(a, b){
                        var a1= a[1], b1= b[1];
                        return b1 - a1;
                    });
                    for(var i= 0, L= A.length; i<L; i++){
                        A[i]= A[i][0];
                    }
                    var obj = {};
                    for(var i= 0, L= A.length; i<L; i++){
                        obj[i] = {key: A[i], value: O[A[i]]};
                    }
                    return obj;
                }

                function getSortedKeys(O){
                    var A= [];
                    for(var p in O){
