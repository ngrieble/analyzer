                    var a1= a[1], b1= b[1];
                    return b1 - a1;
                });
                for(var i= 0, L= A.length; i<L; i++){
                    A[i]= A[i][0];
                }
                var obj = {};
                for(var i= 0, L= A.length; i<L; i++){
